{
  "manifest_version": 3,
  "name": "AI Defender for Children",
  "version": "1.0",
  "description": "Blurs toxic messages on web pages using AI",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["blur.css"]
    }
  ]
}